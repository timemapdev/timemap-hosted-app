{
  "name": "timemap-hosted-app",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "openapi": "npx openapi-typescript@5.4.0 ${VITE_SUPABASE_URL}/rest/v1/?apikey=${VITE_SUPABASE_API_KEY} --output src/openapi/supabase.generated.ts",
    "database-types": "npx supabase gen types typescript --project-id \"${VITE_SUPABASE_ID}\" --schema public > src/openapi/supabase.ts",
    "preview": "vite preview",
    "types": "tsc --noEmit --skipLibCheck"
  },
  "resolutions": {
    "@mui/material": "npm:@mui/joy@latest"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.7",
    "@mui/joy": "latest",
    "@mui/material": "npm:@mui/joy@latest",
    "@supabase/auth-ui-react": "^0.4.2",
    "@supabase/auth-ui-shared": "^0.1.6",
    "@supabase/supabase-js": "^2.33.1",
    "@tanstack/react-form": "0.0.12",
    "@uidotdev/usehooks": "^2.2.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-datasheet-grid": "^4.11.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-router-dom": "^6.15.0",
    "react-select": "^5.7.4",
    "reselect": "^4.1.8",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@dmitrigrabov/eslint-config": "^0.0.1",
    "@testing-library/jest-dom": "^6.1.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-select": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-vitest": "^0.2.8",
    "openapi-typescript": "^6.5.3",
    "prettier": "^3.0.3",
    "supabase": "^1.93.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "0.34.2",
    "vitest-preview": "0.0.1"
  },
  "packageManager": "yarn@3.6.3"
}
